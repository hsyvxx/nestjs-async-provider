{
  "name": "nestjs-async-transform",
  "version": "0.0.1",
  "description": "The way to connect dto to nestjs di system.",
  "author": "",
  "main": "dist/index.js",
  "files": [
    "dist"
  ],
  "license": "MIT",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"{example,lib,test}/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{example,lib,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "prepublishOnly": "nest build nestjs-async-transform"
  },
  "peerDependencies": {
    "@nestjs/common": "^7.5.5",
    "@nestjs/core": "^7.5.5",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^6.6.3"
  },
  "dependencies": {
    "class-transformer": "^0.3.1"
  },
  "devDependencies": {
    "@nestjs/cli": "^7.5.3",
    "@nestjs/common": "^7.5.5",
    "@nestjs/core": "^7.5.5",
    "@nestjs/jwt": "^7.2.0",
    "@nestjs/platform-express": "^7.5.5",
    "@nestjs/schematics": "^7.2.2",
    "@nestjs/testing": "^7.5.5",
    "@types/express": "^4.17.9",
    "@types/jest": "26.0.5",
    "@types/node": "^14.14.9",
    "@types/supertest": "^2.0.10",
    "@typescript-eslint/eslint-plugin": "4.8.2",
    "@typescript-eslint/parser": "4.8.2",
    "eslint": "7.14.0",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-import": "^2.22.1",
    "jest": "26.6.3",
    "prettier": "^2.2.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^6.6.3",
    "supertest": "^6.0.1",
    "ts-jest": "26.4.4",
    "ts-loader": "^8.0.11",
    "ts-node": "^9.0.0",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^4.1.2"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": ".",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "./coverage",
    "testEnvironment": "node",
    "roots": [
      "<rootDir>/example/",
      "<rootDir>/lib/"
    ],
    "moduleNameMapper": {
      "@app/nestjs-async-transform/(.*)": "<rootDir>/lib/$1",
      "@app/nestjs-async-transform": "<rootDir>/lib"
    }
  }
}
